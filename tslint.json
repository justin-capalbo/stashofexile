{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "indent": [true, "tabs", 4],
        "no-console": [false],
        "object-literal-sort-keys": false,
        "one-line": [false],
        "ordered-imports": [false],
        "one-variable-per-declaration": [false]
    },
    "linterOptions": { "exclude": ["**/node_modules/**"]}
}