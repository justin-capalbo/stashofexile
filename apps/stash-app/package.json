{
  "name": "stash-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "get-schema": "apollo service:download --endpoint=http://localhost:4000 ./models/schema/schema.json",
    "make-types": "apollo client:codegen --target=typescript --localSchemaFile=./models/schema/schema.json --includes=./**/*.tsx ./models",
    "codegen": "yarn get-schema && yarn make-types",
    "dev": "next ."
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@emotion/core": "^10.0.6",
    "@emotion/styled": "^10.0.6",
    "@zeit/next-typescript": "^1.1.1",
    "apollo-boost": "^0.1.27",
    "babel-plugin-emotion": "^10.0.6",
    "babel-plugin-module-resolver": "^3.1.3",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "next": "^7.0.2",
    "next-routes": "^1.4.2",
    "next-with-apollo": "^3.3.2",
    "react": "^16.8.0",
    "react-apollo": "^2.4.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@types/next": "^7.0.6",
    "@types/react": "^16.8.1",
    "@types/react-dom": "^16.0.11",
    "apollo": "^2.3.1"
  }
}
